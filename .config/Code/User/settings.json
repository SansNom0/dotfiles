{
    "telemetry.telemetryLevel": "off",
    "editor.fontFamily": "'FreeMono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.enableExperiments": false,
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#8aceff",
        "editorBracketHighlight.foreground2": "#c32be4",
        "editorBracketHighlight.foreground3": "#FF297A",
        "editorBracketHighlight.foreground4": "#3FE086",
        "editorBracketHighlight.foreground5": "#daf846",
        "editorBracketHighlight.unexpectedBracket.foreground": "#8256ef"
    },
    "update.mode": "none",
    "cmake.configureOnOpen": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "omnisharp.useModernNet": false,
    "idf.openOcdConfigs": [
        "interface/ftdi/esp32_devkitj_v1.cfg",
        "target/esp32.cfg"
    ],
    "idf.adapterTargetName": "esp32",
    "thunder-client.httpVersion": "HTTP/2",
    "thunder-client.indentSize": 4,
    "thunder-client.responseLimit": 5
}
